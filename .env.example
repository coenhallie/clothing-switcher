# Supabase Configuration
VITE_SUPABASE_URL=your-supabase-project-url
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Stripe Configuration (for frontend)
VITE_STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key

# OpenRouter API Configuration
# Get your API key from https://openrouter.ai/
# Used for AI image generation via Nano Banana (Gemini 3 Pro Image Preview)
VITE_OPENROUTER_API_KEY=your-openrouter-api-key

# Google AI (Gemini) Direct API Configuration
# Get your API key from https://aistudio.google.com/apikey
# Alternative to OpenRouter â€” talks to Gemini directly without a proxy
VITE_GEMINI_API_KEY=your-gemini-api-key

# Decart AI API Configuration
# Get your API key from https://platform.decart.ai/
# Used for AI video generation only (Lucy Dev I2V model)
VITE_DECART_API_KEY=your-decart-api-key

# Mobile Deep Linking Configuration
# Used for OAuth callbacks and magic link authentication on iOS/Android
# This scheme must match the configuration in src-tauri/tauri.conf.json
VITE_MOBILE_SCHEME=switchfit

# Mobile app bundle identifier (iOS/Android)
# Must match the identifier in src-tauri/tauri.conf.json
VITE_MOBILE_BUNDLE_ID=com.switchfit.studio

# Optional: For development/testing
VITE_APP_ENV=development